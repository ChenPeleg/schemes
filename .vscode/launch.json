{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Racket",
            "type": "node",
            "request": "launch",
            "program": "${file}",
            "runtimeExecutable": "C:/Program Files/Racket/Racket.exe"
        },
        {
            "name": "Run main.rkt",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/main.rkt",
            "runtimeExecutable": "C:/Program Files/Racket/Racket.exe",
            "console": "integratedTerminal"
        },
        {
            "name": "Run main.rkt (Clean Output)",
            "type": "node",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "runtimeExecutable": "C:/Program Files/Racket/Racket.exe",
            "program": "${workspaceFolder}/main.rkt",
            "console": "externalTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}