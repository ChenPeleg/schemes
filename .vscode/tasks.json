{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run Racket File",
            "type": "shell",
            "command": "\"C:/Program Files/Racket/Racket.exe\" ${file}",
            "group": "build",
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "problemMatcher": []
        },
        {
            "label": "Run main.rkt",
            "type": "shell",
            "command": "\"C:/Program Files/Racket/Racket.exe\" \"${workspaceFolder}/main.rkt\"",
            "options": {
                "shell": {
                    "executable": "cmd.exe",
                    "args": [
                        "/c"
                    ]
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new",
                "close": false
            },
            "problemMatcher": []
        }
    ]
}